#set page(width: 58em)
#show math.equation: set align(right)

#box[
  #columns(2, gutter: 2em)[
    #v(0.5em)
    $ "Googolplex" = 10^(10^(100)) $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == 古戈尔数
      初等的大数
    ]
  ]
] <googolplex>

#box[
  #columns(2, gutter: 2em)[
    #v(0.5em)
    $ "Moser" = Delta("megagon", 2) $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == 莫泽数
      通过多边形迭代形成的大数
    ]
  ]
] <moser>

#box[
  #columns(2, gutter: 2em)[
    #v(0.5em)
    $ "G" = g_(64) $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == 葛立恒数
      高德纳表示法可表示的大数
    ]
  ]
] <g64>

#box[
  #columns(2, gutter: 2em)[
    #v(0.5em)
    $ "C" = 3 arrow.r 3 arrow.r 3 arrow.r 3 >> G $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == 康威链式箭头
      将递归从线性拓展到多维得到的大数
    ]
  ]
] <c3333>

#box[
  #columns(2, gutter: 2em)[
    #v(0.5em)
    $ "Gongulus" = {10, 10(100)2} $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == 弓形数
      从多维数组向嵌套数组的强大飞跃
    ]
  ]
] <gongulus>

#box[
  #columns(2, gutter: 2em)[
    #v(0.5em)
    $ cal(G)(4) approx f_(epsilon.alt_0)(3) $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == 古德斯坦序列极限
      表达能力超越皮亚诺公理体系的大数函数
    ]
  ]
] <g4>

#box[
  #columns(2, gutter: 2em)[
    #v(0.5em)
    $ "TREE"(3) approx f_(theta(Omega^omega omega))(3) $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == TREE(3)
      现代大数比较的基准标杆之一
    ]
  ]
] <tree3>

#box[
  #columns(2, gutter: 2em)[
    #v(0.5em)
    $ "SSCG"(3) >> "TREE"(3) $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == Simple Subcubic Graph Nember
      文献中非刻意构造而自然存在的最大数之一
    ]
  ]
] <sscg>

#box[
  #columns(2, gutter: 2em)[
    #v(0.5em)
    $ D^5(99) $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == 洛德数
      可计算数顶峰，极短可终止代码程序最大输出
    ]
  ]
] <loader>

#box[
  #columns(2, gutter: 2em)[
    #v(4em)
    $ "BusyBeaver" $

    #colbreak()

    #block(stroke: (left: white), inset: (left: 1em))[
      == 忙碌...海狸？
      #block(stroke: black)[
        #hide(
          figure(
            image("beaver.png", width: 40%),
          ),
        )
      ] <beaver_image>
    ]
  ]
] <bb>
